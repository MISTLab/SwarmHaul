<?xml version="1.0" ?>
<argos-configuration>

  <!-- ************************* -->
  <!-- * General configuration * -->
  <!-- ************************* -->
  <framework>
    <system threads="1" />
    <experiment length="0"
                ticks_per_second="10"
                random_seed="18" />
  </framework>

  <!-- *************** -->
  <!-- * Controllers * -->
  <!-- *************** -->
  <controllers>

  <buzz_connectivity_controller_kheperaiv id="khivbz"
                                          library="GitHub_repo/Hooks_src/build/libconnectivity_controller">
      <actuators>
        <differential_steering implementation="default" />
        <leds                  implementation="default" medium="leds" />
        <range_and_bearing     implementation="default" />
      </actuators>
      <sensors>
        <!-- <kheperaiv_ground     implementation="rot_z_only" /> -->
        <kheperaiv_proximity  implementation="default" show_rays="false" />
        <range_and_bearing    implementation="medium" medium="rab" show_rays="false"  noise_std_dev="0" />
        <positioning          implementation="default" />
      </sensors>
      <params bytecode_file="buzz_scripts/final1.bo"
              debug_file="buzz_scripts/final1.bdb"
              map_file="GitHub_repo/maps/Comparisions/empty.map"
              map_option="0"
              save_svg_image ="1"
              map_resolution ="0.1" />
      <params>
        <wheel_turning hard_turn_angle_threshold="90"
                       soft_turn_angle_threshold="70"
                       no_turn_angle_threshold="10"
                       max_speed="100" />
      </params>
    </buzz_connectivity_controller_kheperaiv>

  </controllers>

  <!-- ****************** -->
  <!-- * Loop functions * -->
  <!-- ****************** -->
  <loop_functions library="GitHub_repo/Loop_fun_src/build/libplanning_exp.so" 
                  label="Planning"
                  map_file_name="GitHub_repo/maps/Comparisions/empty.map" 
                  map_option="0"
                  robots="30"
                  out_file="OUT1"
                  path="straight"
                  inter_cage_dist="0.45"
                  random_seed_set="18"
                  object_type="5"
                 />
  <!-- Object type enum 
      0 - square object of size (2,2) for 25 robots
      1 - square object of size (3.6,6) for 50 robots 
      2 - square object of size (7.2,18) for 100 robots
      3 - Cloud shape for caging tests
      4 - box_rotation shape for caging tests
      5 - clover shape for caging tests - needs 50 robots -->
     
     
  <!-- *********************** -->
  <!-- * Arena configuration * -->
  <!-- *********************** -->
  <arena size="50, 50, 5" center="0,0,2.5">
    <prototype id="vehicle" movable="true">
      <body position="0,0,0" orientation="0,0,0" />
      <links ref="base">
        <link id="base" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="1,0,0" orientation="45,0,0" />
        <link id="base1" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="0.707107,0.707107,0" orientation="45,0,0" />
        <link id="base2" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="0,1.0,0" orientation="45,0,0" />
        <link id="base3" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="-0.707107,0.707107,0" orientation="45,0,0" />
        <link id="base4" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="-1,0,0" orientation="45,0,0" />
        <link id="base5" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="-0.707107,-0.707107,0" orientation="45,0,0" /> 
        <link id="base6" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="0.0,-1,0" orientation="45,0,0" /> 
        <link id="base7" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="0.707107,-0.707107,0" orientation="45,0,0" /> 
        <!-- <link id="base8" geometry="box" size="0.8, 0.8, 0.3" mass="1"
              position="0.707107,-0.707107,0" orientation="45,0,0" />  -->
        
      </links>
      <joints>
        <joint id="base_01" type="revolute" axis="0,0,1">
          <parent link="base" position="0,0.3,0.1" orientation="45,0,0"/>
          <child link="base1" position="-0.3,-0.3,0.1" orientation="45,0,0"/>
        </joint>
        
        <joint id="base_12" type="revolute" axis="0,0,1">
          <parent link="base1" position="-0.3,0.3,0.1" orientation="0,0,0"/>
          <child link="base2" position="0,-0.3,0.1" orientation="0,0,0"/>
        </joint>
         
        <joint id="base_23" type="revolute" axis="0,0,1">
          <parent link="base2" position="-0.3,0.3,0.1" orientation="0,0,0"/>
          <child link="base3" position="0.3,0,0.1" orientation="0,0,0"/>
        </joint>

        <joint id="base_34" type="revolute" axis="0,0,1">
          <parent link="base3" position="-0.3,0,0.1" orientation="0,0,0"/>
          <child link="base4" position="0.3,0.3,0.1" orientation="0,0,0"/>
        </joint>


        <joint id="base_45" type="revolute" axis="0,0,1">
          <parent link="base4" position="-0.3,-0.3,0.1" orientation="0,0,0"/>
          <child link="base5" position="0,0.3,0.1" orientation="0,0,0"/>
        </joint>


        <joint id="base_56" type="revolute" axis="0,0,1">
          <parent link="base5" position="0,-0.3,0.1" orientation="0,0,0"/>
          <child link="base6" position="-0.3,0.3,0.1" orientation="0,0,0"/>
        </joint>


        <joint id="base_67" type="revolute" axis="0,0,1">
          <parent link="base6" position="0.3,0,0.1" orientation="0,0,0"/>
          <child link="base7" position="-0.3,0.3,0.1" orientation="0,0,0"/>
        </joint>

     <!--    <joint id="base_80" type="revolute" axis="0,0,1">
          <parent link="base" position="-0.3,0,0.1" orientation="0,0,0"/>
          <child link="base7" position="0.3,0.3,0.1" orientation="0,0,0"/>
        </joint>  -->
        
      </joints>
    </prototype>
  	<!-- 14 - 15 robots -->
 <!--   <box id="push_object" size="7.2,18,0.3" mass="5" movable="true">
   <body position="0,0,0" orientation="0,0,0" />
   </box> -->

  <!--  <box id="push_object" size="2,2,0.3" mass="5" movable="true">
   <body position="0,0,0" orientation="0,0,0" />
   </box> -->

  </arena>
  <!-- ******************* -->
  <!-- * Physics engines * -->
  <!-- ******************* -->
     








  <physics_engines>
    <dynamics2d id="dyn2d" />
    <dynamics3d id="dyn3d" iterations="20">
      <floor />
      <gravity g="9.8" />
    </dynamics3d>
  </physics_engines>

  <!-- ********* -->
  <!-- * Media * -->
  <!-- ********* -->
  <media>
    <range_and_bearing id="rab" check_occlusions="false"/>
    <led id="leds" />
    <led id="light" />
  </media>

  <!-- ****************** -->
  <!-- * Visualization * -->
  <!-- ****************** -->
  <visualization>
  <qt-opengl>
      <user_functions label="buzz_qt" />
      <camera>
        <placement idx="0" position="-1.59862,0,4.59862" look_at="0,0,0" lens_focal_length="1" />
        <placement idx="1" position="-1.22369,0,9.18125" look_at="-1.1384,-0.000852933,8.1849" lens_focal_length="20" />
      </camera>s
    </qt-opengl>
    <!-- <qt-opengl /> -->
  </visualization>

</argos-configuration>
